    "equalizer": "workspace:^",
    "linkedin": "workspace:^",
    "openai-analyzer": "workspace:^",
    "plist": "^3.1.0",
    "puppeteer": "21.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/plist": "^3.0.5",
    "typescript": "^5.3.3"
  }
}
