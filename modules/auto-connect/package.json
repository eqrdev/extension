{
  "name": "auto-connect",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --env-file ../../.env ./dist/index.js",
    "build": "tsc --build --clean && tsc --build",
    "build:exec": "yarn dlx pkg dist/index.js -t latest-macos-arm64 -o dist/bin --public",
    "type-check": "tsc --noEmit"
