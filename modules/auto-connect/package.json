    "linkedin": "workspace:^",
    "openai-analyzer": "workspace:^",
    "plist": "^3.1.0",
    "puppeteer": "21.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ui-library": "workspace:^"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
